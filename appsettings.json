{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "EPiServer": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "EPiServer": {
    "Cms": {
      "MappedRoles": {
        "Items": {
          "CmsEditors": {
            "MappedRoles": [ "WebEditors" ]
          },
          "CmsAdmins": {
            "MappedRoles": [
              "WebAdmins, Administrators" ]
          },
          "SearchAdmins": {
            "MappedRoles": [ "SearchAdmins" ]
          }
        }
      }
    }
  }
}